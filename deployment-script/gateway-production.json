{
  "containerDefinitions": [
    {
      "image": "419539186404.dkr.ecr.us-east-1.amazonaws.com/gateway:v_%BUILD_NUMBER%",
      "name": "gateway",
      "cpu": 10,
      "memory": 516,
      "essential": true,
      "portMappings": [
        {
          "protocol": "tcp",
          "containerPort": 3000,
          "hostPort": 4105
        }
      ],
      "environment": [
        {
          "name": "environment",
          "value": "production"
        },
        {
          "name": "service",
          "value": "gateway"
        },
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "DB",
          "value": "%DB%"
        },
        {
          "name": "AWS_ACCESS_KEY_ID",
          "value": "%AWS_ACCESS_KEY_ID%"
        },
        {
          "name": "AWS_SECRET_ACCESS_KEY",
          "value": "%AWS_SECRET_ACCESS_KEY%"
        },
        {
          "name": "REX_PASSWORD",
          "value": "%REX_PASSWORD%"
        },
        {
          "name": "REX_TEST_PASSWORD",
          "value": "%REX_TEST_PASSWORD%"
        },
        {
          "name": "AWS_REGION",
          "value": "us-east-1"
        }
      ]
    }
  ],
  "family": "gateway"
}
